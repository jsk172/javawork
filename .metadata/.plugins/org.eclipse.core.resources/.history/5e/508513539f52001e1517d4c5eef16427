package kr.or.iei.run;

import kr.or.iei.model.User;

public class UserTest {
	public static void main(String[] args) {
		
		User[] user = new User[3];
		
		User user1 = new User("123", "456", "789");
		User user2 = new User("12", "45", "78");
		User user3 = new User("1", "4", "7");
		user1 = user[0];
		user2 = user[1];
		user3 = user[2];
		
		User user4 = new User();
		user4.setId("abc123");
		user4.setPwd("12345");
		user4.setName("김지성");
		
		System.out.println("아이디 : " + user4.getId());
		System.out.println("비밀번호 : " + user4.getPwd());
		System.out.println("이름 : " + user4.getName());
		
		System.out.println(user4.toString());
		
		for(int i=0; i<user.length; i++) {
			System.out.println(user[i]);
		}
	}
}
